<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.*?>

<HBox fx:id="container" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="hour" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="00" />
        <Label fx:id="colon0" text=":" />
        <Label fx:id="minutes" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="00" />
        <Label fx:id="colon1" text=":" />
        <Label fx:id="second" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="00" />
        <Label fx:id="colon2" text=":" />
        <Label fx:id="millSecond" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="000" />
        <Button fx:id="btn" mnemonicParsing="false" styleClass="btn">
            <graphic>
                <FontIcon iconColor="white" iconLiteral="win11-pause" iconSize="16" />
            </graphic>
        </Button>
        <Button fx:id="stopBtn" mnemonicParsing="false" styleClass="btn">
            <graphic>
                <FontIcon iconColor="white" iconLiteral="win11-stop" iconSize="16" />
            </graphic>
        </Button>
    </children>
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
    </padding>
</HBox>
